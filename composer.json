{
    "name": "unionco/syncdb",
    "version": "0.8.0",
    "description": "Library to assist with syncing databases across different environments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abry Rath",
            "email": "abry.rath@union.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "vlucas/phpdotenv": "^2.0||^3.0||^4.0",
        "symfony/process": "^5.2",
        "monolog/monolog": "^2.2",
        "league/container": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "unionco\\syncdb\\": "src/",
            "Test\\": "tests/"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.7"
    },
    "suggest": {
        "monolog/monolog": "Output logging"
    }
}
